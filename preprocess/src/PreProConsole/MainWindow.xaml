<mah:MetroWindow
    x:Class="PreProConsole.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="metroWindow"
    Width="663"
    Height="910"
    MinWidth="618"
    MinHeight="910"
    BorderBrush="#5e4a8a"
    WindowStartupLocation="CenterScreen">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />

            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.Style>
                <Style>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding IsActive, ElementName=metroWindow}" Value="True">
                            <Setter Property="DockPanel.Background" Value="{DynamicResource MahApps.Brushes.WindowTitle}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding IsActive, ElementName=metroWindow}" Value="False">
                            <Setter Property="DockPanel.Background" Value="{DynamicResource MahApps.Brushes.WindowTitle.NonActive}" />
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Grid.Style>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />

            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />

            </Grid.RowDefinitions>

            <Image
                Grid.Row="0"
                Grid.Column="0"
                Height="30"
                Margin="{StaticResource XSmallLeftRightBottomMargin}"
                HorizontalAlignment="Left"
                SnapsToDevicePixels="True"
                Source="/Resources/VisitechLogoWhite.png">
                <Image.Style>
                    <Style>
                        <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
                    </Style>
                </Image.Style>
            </Image>

            <StackPanel
                Grid.Row="1"
                Grid.Column="0"
                Grid.ColumnSpan="2"
                Orientation="Horizontal">
                <Button
                    Name="ClearButton"
                    Margin="{StaticResource SmallLeftMargin}"
                    Click="OnClearClick"
                    Content="Init"
                    FontSize="18"
                    Foreground="White"
                    Style="{StaticResource MahApps.Styles.Button.Calendar.Header}" />

                <Button
                    Name="SaveButton"
                    Margin="{StaticResource SmallLeftMargin}"
                    Click="OnSaveClick"
                    Content="Save"
                    FontSize="18"
                    Foreground="White"
                    Style="{StaticResource MahApps.Styles.Button.Calendar.Header}" />
                <Button
                    Name="LoadButton"
                    Margin="{StaticResource SmallLeftMargin}"
                    Click="OnLoadClick"
                    Content="Load"
                    FontSize="18"
                    Foreground="White"
                    Style="{StaticResource MahApps.Styles.Button.Calendar.Header}" />
                <Button
                    Name="UpdateFidsButton"
                    Margin="{StaticResource SmallLeftRightMargin}"
                    Click="OnUpdateFidsClick"
                    Content="UpdFids"
                    FontSize="18"
                    Foreground="White"
                    Style="{StaticResource MahApps.Styles.Button.Calendar.Header}" />
            </StackPanel>

            <TextBlock
                x:Name="ProductNameTextBox"
                Grid.Row="0"
                Grid.Column="1"
                Grid.ColumnSpan="2"
                Margin="{StaticResource SmallRightMargin}"
                HorizontalAlignment="Right"
                FontSize="25"
                FontWeight="ExtraBold"
                Foreground="White"
                Text="PreProcess Console" />


        </Grid>
        <Separator Grid.Row="1" Margin="0">
            <Separator.Effect>
                <DropShadowEffect
                    BlurRadius="4"
                    ShadowDepth="1"
                    Color="Black" />
            </Separator.Effect>
        </Separator>
        <Border Grid.Row="2" Margin="0">

            <Grid Margin="{StaticResource SmallLeftTopRightBottomMargin}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Border
                    Grid.Row="0"
                    Grid.ColumnSpan="2"
                    Margin="10">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>

                        <StackPanel
                            Grid.Row="0"
                            Grid.Column="1"
                            Orientation="Horizontal">
                            <RadioButton
                                Name="GerberButton"
                                Margin="5"
                                Checked="GerberButton_Checked"
                                Content="Gerber"
                                IsChecked="True" />
                            <RadioButton
                                Name="GdsButton"
                                Margin="5"
                                Checked="GdsButton_Checked"
                                Content="GDSII" />
                            <RadioButton
                                Name="OdbButton"
                                Margin="5"
                                Checked="OdbButton_Checked"
                                Content="ODB++" />

                        </StackPanel>
                        <Button
                            Name="PreConvertButton"
                            Grid.Row="0"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            Margin="5"
                            HorizontalAlignment="Right"
                            Click="OnPrepareClick"
                            Content="PreConvert"
                            IsEnabled="false" />
                        <Label
                            Grid.Row="1"
                            Margin="5"
                            HorizontalAlignment="Left"
                            Content="Input file:" />
                        <TextBox
                            x:Name="GerberInput"
                            Grid.Row="1"
                            Grid.Column="1"
                            Margin="5,5,0,5" />
                        <Button
                            Grid.Row="1"
                            Grid.Column="2"
                            Width="28"
                            Height="28"
                            Margin="0,5,5,5"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Click="OnGerberBrowsClick"
                            Style="{StaticResource BrowseButton}" />

                        <Label
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="5"
                            HorizontalAlignment="Left"
                            Content="Output dir:" />
                        <TextBox
                            x:Name="OutputDir"
                            Grid.Row="2"
                            Grid.Column="1"
                            Margin="5,5,0,5" />
                        <Button
                            Grid.Row="2"
                            Grid.Column="2"
                            Width="28"
                            Height="28"
                            Margin="0,5,5,5"
                            HorizontalAlignment="Left"
                            Click="OnOutputBrowsClick"
                            Style="{StaticResource BrowseButton}" />

                        <Label
                            Grid.Row="3"
                            Grid.Column="0"
                            Margin="5"
                            HorizontalAlignment="Left"
                            Content="Work dir:" />
                        <TextBox
                            x:Name="WorkDir"
                            Grid.Row="3"
                            Grid.Column="1"
                            Margin="5,5,0,5" />
                        <Button
                            Grid.Row="3"
                            Grid.Column="2"
                            Width="28"
                            Height="28"
                            Margin="0,5,5,5"
                            HorizontalAlignment="Left"
                            Click="OnWorkBrowsClick"
                            Style="{StaticResource BrowseButton}" />

                        <Label
                            Grid.Row="4"
                            Grid.Column="0"
                            Margin="5"
                            HorizontalAlignment="Left"
                            Content="Suffix:" />
                        <TextBox
                            x:Name="SuffixEdit"
                            Grid.Row="4"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            Margin="5" />

                        <Grid Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" x:Name="OdbGrid">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>
                            
                            <Label
                                Margin="5"
                                Content="ODB Step:" 
                                Grid.Column="0"/>
                            <TextBox
                                x:Name="OdbStepEdit"
                                Height="23"
                                Margin="9,5,5,5"
                                TextWrapping="Wrap" 
                                Grid.Column="1"/>

                            <Label
                                Margin="5"
                                Content="ODB Layer:" 
                                Grid.Column="2"/>
                            <TextBox
                                x:Name="OdbLayerEdit"
                                Height="23"
                                Margin="5"
                                TextWrapping="Wrap" 
                                Grid.Column="3"/>
                            <Button
                                Width="28"
                                Height="28"
                                Margin="0,5,5,5"
                                HorizontalAlignment="Left"
                                Name="ODBScannerButton"
                                Click="OnODBScannerClick"
                                Style="{StaticResource BrowseButton}"
                                Grid.Column="4"/>
                        </Grid>
                    </Grid>
                </Border>
                <TabControl
                    Grid.Row="1"
                    Grid.Column="0"
                    Margin="10,20,10,0"
                    Style="{x:Null}">
                    <TabItem Header="Image">
                        <StackPanel Margin="10,10,10,0" Orientation="Horizontal">
                            <StackPanel Margin="10,10,10,0" Orientation="Vertical">
                                <StackPanel Margin="5" Orientation="Vertical">
                                    <RadioButton
                                        Name="AutoWindowRadioButton"
                                        Margin="5"
                                        Checked="OnAutoWindowChecked"
                                        Content="AutoWindow"
                                        GroupName="GerberConvertWindowGroup" />
                                    <RadioButton
                                        Name="FixedWindowRadioButton"
                                        Margin="5"
                                        Checked="OnFixedWindowChecked"
                                        Content="FixedWindow"
                                        GroupName="GerberConvertWindowGroup" />
                                </StackPanel>
                                <StackPanel Margin="5,5,5,0" Orientation="Vertical">
                                    <CheckBox
                                        x:Name="InverseCheckBox"
                                        Margin="5"
                                        Content="InvertColors" />
                                    <CheckBox
                                        x:Name="InverseFrameCheckBox"
                                        Margin="5"
                                        Content="InvertFrame" />
                                    <CheckBox
                                        x:Name="FlipHCheckBox"
                                        Margin="5"
                                        Content="HorizontalFlip" />
                                    <CheckBox
                                        x:Name="FlipVCheckBox"
                                        Margin="5"
                                        Content="VerticalFlip" />
                                    <CheckBox
                                        x:Name="MakePreviewCheckBox"
                                        Margin="5"
                                        Content="MakePreview(old)" />
                                    <CheckBox
                                        x:Name="NegativeYDirectionCheckBox"
                                        Margin="5"
                                        Content="NegativeYDirection" />
                                    <CheckBox
                                        x:Name="EdgeBlendingCheckBox"
                                        Margin="5"
                                        Content="EdgeBlending" />
                                    <CheckBox
                                        x:Name="MTCCheckBox"
                                        Margin="5"
                                        Content="MTC" />
                                    <CheckBox
                                        x:Name="SmoothingCheckBox"
                                        Margin="5"
                                        Content="Smoothing" />
                                </StackPanel>
                            </StackPanel>
                            <WrapPanel Margin="10" Orientation="Vertical">
                                <StackPanel
                                    x:Name="OffX"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="OffsetX (mm):" />
                                    <TextBox
                                        x:Name="OffsetX"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel
                                    x:Name="OffY"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="OffsetY (mm):" />
                                    <TextBox
                                        x:Name="OffsetY"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel
                                    x:Name="Llx"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="WindowLLx (mm):" />
                                    <TextBox
                                        x:Name="WindowLLx"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel
                                    x:Name="Lly"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="WindowLLy (mm):" />
                                    <TextBox
                                        x:Name="WindowLLy"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel
                                    x:Name="Urx"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="WindowURx (mm):" />
                                    <TextBox
                                        x:Name="WindowURx"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel
                                    x:Name="Ury"
                                    HorizontalAlignment="Left"
                                    Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="WindowURy (mm):" />
                                    <TextBox
                                        x:Name="WindowURy"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="ErosionX (mm):" />
                                    <TextBox
                                        x:Name="ErosionX"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="ErosionY (mm):" />
                                    <TextBox
                                        x:Name="ErosionY"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel Name="GlobalFiducialSP" HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="GlobalFiducial:" />
                                    <TextBox
                                        x:Name="GlobalFiducial"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel Name="LocalFiducialSP" HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="LocalFiducial:" />
                                    <TextBox
                                        x:Name="LocalFiducial"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel Name="RotationSP" HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="RotationCCW (deg):" />
                                    <TextBox
                                        x:Name="Rotation"
                                        Width="80"
                                        Height="23"
                                        Margin="5" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="TableLimitX (mm):" />
                                    <TextBox
                                        x:Name="TableLimitXEdit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="WhiteLines" />
                                    <TextBox
                                        x:Name="WhiteLinesEdit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal">
                                    <Label
                                        Width="109"
                                        Margin="5"
                                        Content="ImageFrame:" />
                                    <ComboBox
                                        Name="ImageFrameComboBox"
                                        Margin="5"
                                        ItemsSource="{Binding Path=ImageFrameList}" />
                                </StackPanel>
                            </WrapPanel>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Photoheads">
                        <StackPanel Margin="10" Orientation="Horizontal">
                            <Grid Margin="10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <CheckBox
                                    x:Name="PhotoheadRotatedCheckBox"
                                    Grid.Row="0"
                                    Grid.ColumnSpan="2"
                                    Margin="10"
                                    Content="PhotoheadRotated" />
                                <Label
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="NumberOfPhotoheads:" />
                                <ComboBox
                                    Name="NumberOfPhotoheadsCB"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="5"
                                    ItemsSource="{Binding Path=NumberOfPhotoheadsList}"
                                    SelectionChanged="NumPHSelectionChanged" />
                                <Label
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="PhotoheadPitch:" />
                                <ComboBox
                                    Name="PhotoheadPitchCB"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="5"
                                    ItemsSource="{Binding Path=PhotoheadPitchList}"
                                    SelectionChanged="PhPitchSelectionChanged" />
                                <Label
                                    Grid.Row="3"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="ProductID:" />
                                <ComboBox
                                    Name="ProductIDComboBox"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Margin="5"
                                    ItemsSource="{Binding Path=ProductIDList}"
                                    SelectionChanged="ProductIDChangedChanged" />
                            </Grid>
                            <WrapPanel Margin="10" Orientation="Vertical">
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist2Label"
                                        Margin="5"
                                        Content="NominalPHDist #2 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist2Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist3Label"
                                        Margin="5"
                                        Content="NominalPHDist #3 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist3Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist4Label"
                                        Margin="5"
                                        Content="NominalPHDist #4 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist4Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist5Label"
                                        Margin="5"
                                        Content="NominalPHDist #5 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist5Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist6Label"
                                        Margin="5"
                                        Content="NominalPHDist #6 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist6Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist7Label"
                                        Margin="5"
                                        Content="NominalPHDist #7 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist7Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Label
                                        x:Name="NominalPhotoheadDist8Label"
                                        Margin="5"
                                        Content="NominalPHDist #8 (mm):" />
                                    <TextBox
                                        x:Name="NominalPhotoheadDist8Edit"
                                        Width="80"
                                        Height="23"
                                        Margin="5"
                                        TextWrapping="Wrap" />
                                </StackPanel>
                            </WrapPanel>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Preview" Name="PreviewTab">
                        <StackPanel Margin="10" Orientation="Horizontal">
                            <StackPanel Margin="10" Orientation="Vertical">
                                <CheckBox
                                    x:Name="EnableAdvPreviewCheckBox"
                                    Margin="5"
                                    Content="EnableAdvPreview" />
                                <CheckBox
                                    x:Name="FastModeCheckBox"
                                    Margin="5"
                                    Content="FastMode" />
                                <CheckBox
                                    x:Name="ShowPaneldataCheckBox"
                                    Margin="5"
                                    Content="ShowPaneldata" />
                                <CheckBox
                                    x:Name="ShowLinesCheckBox"
                                    Margin="5"
                                    Content="ShowLines" />
                                <CheckBox
                                    x:Name="VerticalFlipCheckBox"
                                    Margin="5"
                                    Content="VerticalFlip" />
                                <CheckBox
                                    x:Name="DrawFilledRectanglesCheckBox"
                                    Margin="5"
                                    Content="DrawFilledRectangles" />
                                <CheckBox
                                    x:Name="CollisionDetectionCheckBox"
                                    Margin="5"
                                    Content="CollisionDetection (Beta)" />
                                <CheckBox
                                    x:Name="SaveAsPngCheckBox"
                                    Margin="5"
                                    Content="SaveAsPng (slower)" />
                            </StackPanel>
                            <StackPanel Margin="10" Orientation="Vertical" />
                            <Grid Margin="10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Label
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="PreDrawFactor:" />
                                <TextBox
                                    x:Name="PreDrawFactorEdit"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Width="56"
                                    Height="23"
                                    Margin="5" />
                                <Label
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="PostDrawFactor:" />
                                <TextBox
                                    x:Name="PostDrawFactorEdit"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Width="56"
                                    Height="23"
                                    Margin="5" />
                                <Label
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="DrawLineWidth:" />
                                <TextBox
                                    x:Name="DrawLineWidthEdit"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Width="56"
                                    Height="23"
                                    Margin="5" />
                                <Label
                                    Grid.Row="3"
                                    Grid.Column="0"
                                    Margin="5"
                                    Content="MaxMemory (MB):" />
                                <TextBox
                                    x:Name="MaxMemoryEdit"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Width="56"
                                    Height="23"
                                    Margin="5" />
                            </Grid>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Static PanelData" Name="StaticPaneldataTab">
                        <Grid Margin="10">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="5"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center">
                                Gerber file:
                            </TextBlock>
                            <TextBox
